# ZANTARA Webapp Environment Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# API Configuration
VITE_API_BASE_URL=https://zantara-v520-production-1064094238013.europe-west1.run.app
VITE_PROXY_URL=https://zantara-web-proxy-himaadsxua-ew.a.run.app/api/zantara

# API Keys (NEVER expose in client-side code)
# These should only be used by proxy/BFF server-side
ZANTARA_API_KEY_INTERNAL=your-internal-key-here
ZANTARA_API_KEY_EXTERNAL=your-external-key-here

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# Environment
NODE_ENV=development
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_STREAMING=true
VITE_ENABLE_VOICE=true
VITE_ENABLE_VIRTUALIZATION=true